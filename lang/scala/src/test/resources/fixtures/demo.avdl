@namespace("com.sumologic.org.api")
protocol Org {
  enum Species {
    Dog, Hamster
  }

  record Pet {
    Species species;
  }

  record Person {
    string name;
    int age;
    map<Pet> pets;
    union { string, double } onion;
  }

  Person createPerson(string name, int age, map<Pet> pets, union { string, double } onion);
  void removePerson(Person person);
}
